{
  "name": "angular-template",
  "version": "0.0.1",
  "description": "Angular 4 + Webpack 3 template",
  "keywords": [
    "angular",
    "template"
  ],
  "author": "MagoraSystems frontend team (https://magora-systems.com)",
  "contributors": [
    "Andrey Shabanov <shabanov@magora-systems.com>"
  ],
  "homepage": "https://magora-systems.com",
  "license": "MIT",
  "scripts": {
    "build:aot:prod": "npm run clean:dist && npm run clean:aot && cross-env BUILD_AOT=1 npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
    "build:dev": "npm run clean:dist && npm run webpack -- --config config/webpack.dev.js --progress --profile",
    "build:prod": "npm run clean:dist-prod && npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:dev",
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "clean:dist-prod": "npm run rimraf -- dist-prod",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean --force && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "node": "node",
    "preclean:install": "npm run clean",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --hotOnly",
    "server:dev": "npm run webpack-dev-server -- --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start": "npm run server:dev",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
  "dependencies": {
    "@angular/animations": "^5.2.2",
    "@angular/common": "^5.2.2",
    "@angular/compiler": "^5.2.2",
    "@angular/core": "^5.2.2",
    "@angular/forms": "^5.2.2",
    "@angular/http": "^5.2.2",
    "@angular/platform-browser": "^5.2.2",
    "@angular/platform-browser-dynamic": "^5.2.2",
    "@angular/platform-server": "^5.2.2",
    "@angular/router": "^5.2.2",
    "@angularclass/hmr": "~2.1.3",
    "@angularclass/hmr-loader": "^3.0.4",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0",
    "core-js": "^2.5.3",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.6.6",
    "@angular/compiler-cli": "^5.2.2",
    "@types/jasmine": "2.8.6",
    "@types/node": "8.0.53",
    "@types/source-map": "^0.5.7",
    "@types/uglify-js": "^2.6.30",
    "@types/webpack": "^3.8.4",
    "add-asset-html-webpack-plugin": "^2.1.2",
    "angular2-template-loader": "^0.6.2",
    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "^3.4.1",
    "codelyzer": "^4.0.2",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "exports-loader": "^0.6.4",
    "expose-loader": "^0.7.4",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "ng-router-loader": "^2.1.0",
    "ngc-webpack": "^3.2.2",
    "npm-run-all": "^4.1.1",
    "optimize-js-plugin": "0.0.4",
    "parse5": "^3.0.3",
    "preload-webpack-plugin": "^2.2.0",
    "pug": "^2.0.0-rc.4",
    "pug-html-loader": "^1.1.5",
    "raw-loader": "0.5.1",
    "rimraf": "~2.6.2",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "source-map-loader": "^0.2.3",
    "string-replace-loader": "~1.3.0",
    "style-loader": "^0.19.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "to-string-loader": "^1.1.5",
    "ts-node": "^4.1.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "tslint-loader": "^3.5.3",
    "typescript": "~2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^2.9.7",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-merge": "~4.1.1"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  }
}
